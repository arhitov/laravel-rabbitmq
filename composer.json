{
  "name": "clgsru/laravel-rabbitmq",
  "type": "library",
  "description": "The message broker on RabbitMQ for Laravel Framework.",
  "keywords": ["laravel"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/clgsru/laravel-rabbitmq/issues",
    "source": "https://github.com/clgsru/laravel-rabbitmq",
    "packagist": "https://packagist.org/packages/clgsru/laravel-rabbitmq"
  },
  "authors": [
    {
      "name": "Alexander Arhitov",
      "email": "clgsru@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4|^8.0",
    "php-amqplib/php-amqplib": "^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "laravel/framework": "^8.75",
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "ClgsRu\\LaravelRabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ClgsRu\\LaravelRabbitMQ\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ClgsRu\\LaravelRabbitMQ\\Providers\\LaravelRabbitMQServiceProvider"
      ]
    }
  },
  "scripts": {
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
